{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Seed": {
    "AdminEmail": "admin@system.com",
    "AdminPassword": "Admin123!"
  },
  "JwtSettings": {
    "Issuer": "YourCompany",
    "Audience": "YourCompanyAudience",
    "SecretKey": "EstaEsUnaClaveSuperSecretaMuySegura123!",
    "TokenLifetimeMinutes": 60
  },

  "ApiSettings": {
    "PageSizeDefault": 20,
    "MaxPageSize": 100
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
